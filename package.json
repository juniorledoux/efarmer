{
  "name": "backend",
  "version": "1.0.0",
  "description": "Contient toute la partie backend",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "client-install": "cd client && npm install",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "client-build": "cd client && npm run build",
    "install-netlify": "cd client && npm install netlify-cli -g",
    "netlify-deploy": "cd client && netlify deploy --prod",
    "heroku-deploy": "git push heroku main",
    "depoy-prod": "npm run netlify-deploy && npm run heroku-deploy",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "concurrently": "^7.3.0",
    "connect-mongodb-session": "^3.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.4.5",
    "mongoose-unique-validator": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.19",
    "react-router-dom": "^6.3.0"
  }
}
